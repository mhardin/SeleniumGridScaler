{
    "capabilities":
        [
            {
                "browserName": "firefox",
                "maxInstances": <MAX_SESSION_FIREFOX>,
                "seleniumProtocol": "WebDriver"
            },
            {
                "browserName": "chrome",
                "maxInstances": <MAX_SESSION_CHROME>,
                "seleniumProtocol": "WebDriver"
            }
        ],
        "proxy": "org.openqa.grid.selenium.proxy.DefaultRemoteProxy",
        "maxSession": <MAX_SESSION>,
        "register": true,
        "registerCycle": 5000,
        "hub": "<HOST_NAME>",
        "custom": {uuid=<UUID>,instanceId=<INSTANCE_ID>,createdDate=<CREATED_DATE>,createdBrowser=<CREATED_BROWSER>,createdOs=<CREATED_OS>}
}
